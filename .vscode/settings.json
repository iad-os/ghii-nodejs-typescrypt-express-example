// Place your settings in this file to overwrite default and user settings.
{
  "window.title": "ghii-ts-express-example${separator}${dirty}${activeEditorShort}${separator}${appName}",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript", "typescript"],
  "editor.formatOnSave": false,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/coverage": true
  },
  "typescript.referencesCodeLens.enabled": true,
  "appService.zipIgnorePattern": [".vscode{,/**}"],
  "appService.deploySubpath": "",
  "files.eol": "\n",
  "cSpell.words": ["Millisecs", "ghii", "nats", "openid", "pino"],
  "errorLens.annotationPrefix": ["üí•", "‚ö†Ô∏è", "‚Ñπ", "üí°"],
  "errorLens.enabledDiagnosticLevels": ["error", "warning"],
  "errorLens.gutterIconsEnabled": true,
  "errorLens.fontSize": "12",
  "errorLens.fontStyleItalic": true,
  "errorLens.margin": "30ch"
}
